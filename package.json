{
  "name": "@widcardw/remark-callouts",
  "type": "module",
  "version": "0.1.2",
  "description": "",
  "homepage": "https://github.com/widcardw/remark-callouts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/widcardw/remark-callouts.git"
  },
  "bugs": "https://github.com/widcardw/remark-callouts/issues",
  "scripts": {
    "test": "vitest --run",
    "build": "tsup ./src/index.ts --format cjs,esm --dts && postcss ./src/index.css > ./dist/index.css",
    "release": "bumpp && npm publish --access public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "style": [
    "dist/index.css"
  ],
  "files": [
    "dist"
  ],
  "keywords": [
    "remark",
    "callouts"
  ],
  "author": "widcardw",
  "license": "MIT",
  "devDependencies": {
    "@types/mdast": "^4.0.0",
    "@types/unist": "^3.0.0",
    "bumpp": "^9.1.1",
    "cssnano": "^6.0.1",
    "mdast-util-from-markdown": "^2.0.0",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "rehype-stringify": "^9.0.3",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "unified": "^10.1.2",
    "unist-util-visit": "^5.0.0",
    "vitest": "^0.33.0"
  }
}
